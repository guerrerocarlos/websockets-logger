{
  "name": "websockets-logger",
  "version": "0.1.1",
  "description": "Lightweight WebSocket logger that forwards console output to a centralized hub.",
  "license": "MIT",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
