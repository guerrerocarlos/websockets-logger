  "sideEffects": false,
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist",
    "lint": "tsc --noEmit"
  },
  "dependencies": {},
  "devDependencies": {
    "rimraf": "^5.0.5",
    "tsup": "^7.2.0",
    "typescript": "^5.3.3"
  }
}